<% include ./partials/header.ejs %>
<div class="w-full min-h-screen bg-zinc-800 px-10 py-8 text-white">
  <div class="cards gap-10 flex flex-wrap px-10 mt-10">
    <% posts.forEach((elem)=>{ %>
    <div class="card w-32 mt-5">
      <div class="w-full h-40 bg-sky-200 rounded-lg overflow-hidden">
        <img
          class="w-full h-full object-cover"
          src="/images/uploads/<%= elem.image %>"
          alt=""
        />
      </div>
      <h5 class="px-1 text-sm mt-2 font-medium truncate"><%= elem.title %></h5>
    </div>
    <% }) %>
  </div>
</div>

<%include ./partials/footer.ejs %>
